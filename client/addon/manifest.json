{
   "action": {
      "default_popup": "popup/popup.html"
   },
   "description": "A HSLU HackSTAIR project in the middle of October 2023.",
   "host_permissions": [ "https://twitter.com/*", "https://x.com/*" ],
   "icons": {
      "128": "icons/Jira128.png",
      "16": "icons/Jira16.png",
      "32": "icons/Jira32.png",
      "48": "icons/Jira48.png"
   },
   "manifest_version": 3,
   "name": "safe_",
   "permissions": [ "tabs" ],
   "version": "0.0.0.1",
   "content_scripts": [
      {
         "matches": [ "https://twitter.com/*", "https://x.com/*" ],
         "js": [ "content_scripts/twitter.js" ],
         "css" : [ "content_scripts/twitter.css" ]
      }
   ]
}
